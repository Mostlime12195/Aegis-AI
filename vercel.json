{
  "rewrites": [{ "source": "/api/:path*", "destination": "/api/:path*" }],
  "routes": [
    {
      "src": "/(.*)\\.(js|css|png|svg|jpg|jpeg|gif|webp|woff|woff2|ttf|ico)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    }
  ]
}
